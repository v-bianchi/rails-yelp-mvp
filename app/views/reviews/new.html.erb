<div class="container">
  <%= simple_form_for [@restaurant, @review] do |f| %>
    <div class="form-inputs">
      <%= f.input :content %>
      <p>Rating</p>
      <%= f.select :rating, (0..5) %>
    </div>

    <div class="form-actions">
      <%= f.button :submit %>
    </div>
  <% end %>
</div>


